        Text1(i).Text = " "
    Next i
    
End Sub

Private Sub C_Iniciar_Click()
    
    Dim pos_actual As posi
    Dim pos_anterior As posi
    
    Reiniciar_pictures
    
    C_Iniciar.Enabled = False
    C_Reiniciar.Enabled = True
    mov = 0 'Posem els moviments a 0
    segon = False
    Buscar_Posicions_Possibles 'Busquem inicialment les posicions possibles
    Omplir_Matriu_Textos 'Omplim la matriu de textos
    Omplir_matriu_picture
    segon = True
    
    
    pos_actual = Triar_Posicio_Inici 'Busquem aleatoriament la pos d'inici
    m(pos_actual.p_x, pos_actual.p_y) = 0
    
    mov = mov + 1
    Omplir_Matriu_Textos
    Omplir_matriu_picture
    pos_anterior.p_x = pos_actual.p_x
    pos_anterior.p_y = pos_actual.p_y
   
 
    While pos_actual.numero <> 0 Or pos_actual.final = False
        
            
        pos_actual = Buscar_Millor_Posicio(pos_anterior.p_x, pos_anterior.p_y)
           
        
        'MsgBox "X act: " & pos_actual.p_x & " Y act: " & pos_actual.p_y & " X ant: " & pos_anterior.p_x & " Y ant: " & pos_anterior.p_y
        If pos_actual.final = False Then
            m(pos_actual.p_x, pos_actual.p_y) = 0
            Omplir_matriu_picture
            mov = mov + 1
            Buscar_Posicions_Possibles
            Omplir_Matriu_Textos
            pos_anterior.p_x = pos_actual.p_x
            pos_anterior.p_y = pos_actual.p_y
            
        End If
        
    Wend
    Omplir_matriu_picture
    MsgBox "Final!!! has fet: " & mov + 1 & " moviments"
    L_Moviments.Caption = mov + 1
End Sub

Private Sub C_Reiniciar_Click()

    C_Iniciar.Enabled = True
    C_Reiniciar.Enabled = False
    Reiniciar_pictures
    borrar_textos 'posem en blanc la matriu de textes
    Posar_Matriu_a_0
    mov = 0
    Omplir_Matriu_Textos
    
    
End Sub

